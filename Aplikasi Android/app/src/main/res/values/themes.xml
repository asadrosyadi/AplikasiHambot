    <style name="Theme.Fuzzybayes.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Fuzzybayes.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
